                                                    @if (!string.IsNullOrEmpty(editedWellName) && wells[wKey] != null && !string.IsNullOrEmpty(wells[wKey].Name) && editedWellName.Equals(wells[wKey].Name))
                                                    {
                                                        foreach (int wbKey in wellBores.Keys)

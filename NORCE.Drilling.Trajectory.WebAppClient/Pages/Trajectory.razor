                                                            if (wells[wKey].ID == wellBores[wbKey].WellID)
                                                            {
                                                                @if (!string.IsNullOrEmpty(editedWellBoreName) && wellBores[wbKey] != null && !string.IsNullOrEmpty(wellBores[wbKey].Name) && editedWellBoreName.Equals(wellBores[wbKey].Name))
                                                                {
